@{
}
@model List<StudentPortal.web.Models.Entities.Student>
<h1>Student List</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Subscribed</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var student in Model)
        {
           <tr>
               <td> @student.Id</td>
               <td> @student.Name</td >
               <td> @student.Phone</td >
               <td> @student.Email</td>
               <td> @student.Subscribed</td >
               <td>
                   <a class= "btn btn-primary" asp-controller="StudentsController1" asp-action="Edit" asp-route-id="@student.Id">Edit</a>
                   </td>
           </tr> 
        }
        </tbody>
</table>